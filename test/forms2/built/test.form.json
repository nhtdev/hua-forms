{
	"method": "post",
	"fields": [
		{
			"name": "name",
			"label": "Nom",
			"type": "text",
			"formatters": [
				{
					"type": "trim"
				}
			],
			"rules": [
				{
					"type": "required",
					"message": "Saisissez votre nom"
				},
				{
					"type": "maxlength",
					"maxlength": 60
				}
			]
		},
		{
			"name": "gender",
			"label": "Gender",
			"type": "select",
			"rules": [
				{
					"type": "required"
				},
				{
					"type": "inArray",
					"values": ["M", "F"]
				}
			]
		}
	],
	"submits": [
		{
			"name": "ok",
			"label": "OK"
		}
	]
}